{
  "name": "drizzle-kit",
  "version": "0.21.4",
  "repository": "https://github.com/drizzle-team/drizzle-kit-mirror",
  "author": "Drizzle Team",
  "license": "MIT",
  "bin": {
    "drizzle-kit": "./bin.cjs"
  },
  "scripts": {
    "payload": "tsx ./dev/payload.ts",
    "migrate:old": "drizzle-kit generate:mysql",
    "cli": "tsx ./src/cli/index.ts",
    "test": "vitest",
    "build": "rm -rf ./dist && tsx build.ts && cp package.json dist/ && attw --pack dist",
    "build:dev": "rm -rf ./dist && tsx build.dev.ts && tsc -p tsconfig.cli-types.json && chmod +x ./dist/index.cjs",
    "packit": "pnpm build && cp package.json dist/ && cd dist && pnpm pack",
    "tsc": "tsc -p tsconfig.build.json",
    "pub": "cp package.json readme.md dist/ && cd dist && npm publish"
  },
  "dependencies": {
    "@esbuild-kit/esm-loader": "^2.5.5",
    "commander": "^9.4.1",
    "env-paths": "^3.0.0",
    "esbuild": "^0.19.7",
    "esbuild-register": "^3.5.0",
    "glob": "^8.1.0",
    "hanji": "^0.0.5",
    "json-diff": "0.9.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@aws-sdk/client-rds-data": "^3.556.0",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@electric-sql/pglite": "^0.1.3",
    "@hono/node-server": "^1.9.0",
    "@hono/zod-validator": "^0.2.1",
    "@libsql/client": "^0.4.2",
    "@neondatabase/serverless": "^0.9.1",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@planetscale/database": "^1.16.0",
    "@types/better-sqlite3": "^7.6.4",
    "@types/dockerode": "^3.3.28",
    "@types/glob": "^8.1.0",
    "@types/json-diff": "^1.0.3",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.11.15",
    "@types/pg": "^8.10.7",
    "@types/pluralize": "^0.0.33",
    "@types/semver": "^7.5.5",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vercel/postgres": "^0.8.0",
    "ava": "^5.1.0",
    "better-sqlite3": "^9.4.3",
    "camelcase": "^7.0.1",
    "chalk": "^5.2.0",
    "dockerode": "^3.3.4",
    "dotenv": "^16.0.3",
    "drizzle-kit": "0.20.18",
    "drizzle-orm": "0.30.9-f9be0ab",
    "esbuild-node-externals": "^1.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "get-port": "^6.1.2",
    "hono": "^4.1.5",
    "minimatch": "^7.4.3",
    "mysql2": "2.3.3",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "pluralize": "^8.0.0",
    "postgres": "^3.4.4",
    "prettier": "^2.8.1",
    "semver": "^7.5.4",
    "superjson": "^2.2.1",
    "tsup": "^8.0.2",
    "tsx": "^3.12.1",
    "typescript": "^5.4.3",
    "uuid": "^9.0.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0",
    "wrangler": "^3.22.1",
    "ws": "^8.16.0",
    "zx": "^7.2.2"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.mts",
        "default": "./index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "types": "./index.d.mts",
      "default": "./index.mjs"
    },
    "./payload": {
      "import": {
        "types": "./payload.d.mts",
        "default": "./payload.mjs"
      },
      "require": {
        "types": "./payload.d.ts",
        "default": "./payload.js"
      },
      "types": "./payload.d.mts",
      "default": "./payload.mjs"
    }
  },
  "pnpm": {
    "patchedDependencies": {
      "difflib@0.2.4": "patches/difflib@0.2.4.patch"
    }
  }
}
